#include<stdio.h>
int main(){
    int common,a,i,num1,num2;
    printf("Enter the first number:");
    scanf("%d",&num1);
    printf("Enter the second number:");
    scanf("%d",&num2);
    if(num1<num2)
        a=num1;
    else
        a=num2;
    for(i=1;i<=a;i++){
        if(num1%i==0){
            if(num2%i==0){
                common=i;
    }}}
    printf("The greatest common divisor is: %d",common);
 return 0;
}
